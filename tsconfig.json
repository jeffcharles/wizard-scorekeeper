{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["dom", "es5", "es2015", "scripthost"],
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "outDir": "./dist/",
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5"
  },
  "include": [
    "src/**/*"
  ]
}
